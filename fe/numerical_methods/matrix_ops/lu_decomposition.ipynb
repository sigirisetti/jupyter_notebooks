    "import scipy.linalg as linalg\n",
    "import numpy as np\n",
    "\n",
    "A = np.array([[2., 1., 1.],\n",
    "              [1., 3., 2.],\n",
    "              [1., 0., 0.]])\n",
    "B = np.array([4., 5., 6.])\n",
    "\n",
    "LU = linalg.lu_factor(A)\n",
    "x = linalg.lu_solve(LU, B)\n",
    "\n",
    "print(x)\n",
    "\n",
    "P, L, U = linalg.lu(A)\n",
    "print(P)\n",
    "print(L)\n",
    "print(U)\n"
   ]
